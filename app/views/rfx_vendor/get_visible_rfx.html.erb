<p id="notice"><%= notice %></p>

  <h1>询价单清单 - <small><%= @current_vendor.full_name unless @current_vendor.nil?%></small></h1>

<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th>序号</th>
      <th>开始时间</th>
      <th>结束时间</th>
      <th>采购分组</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @rfxes.each do |rfx| %>
      <tr>
        <td><%= rfx.SN %></td>
        <td><%= rfx.begin_dt %></td>
        <td><%= rfx.end_dt %></td>
        <td><%= rfx.sourcing_org.full_name %></td>
        <td><%= link_to '报价', rfx_vendors_open_rfx_items_path(rfx) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


