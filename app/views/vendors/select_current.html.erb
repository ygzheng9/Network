<h1>当前供应商：</h1>

<%= @vendor.full_name unless @vendor.nil? %>

<%= form_tag vendors_set_change_path, method: :PUT do %>

  <%= select_tag "vendor_id",
            options_from_collection_for_select(@vendors, "id", "name", "1"),
            class: "form-control" %>
  <%= submit_tag "切换供应商", class: "btn btn-default" %>

<% end %>